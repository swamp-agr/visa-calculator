{
  "name": "visa-calculator",
  "version": "0.1.0",
  "description": "Visa calculations",
  "main": "support/entry.js",
  "keywords": [
    "pux",
    "purescript-pux",
    "boilerplate",
    "starter-app"
  ],
  "scripts": {
    "postinstall": "bower cache clean && bower install",
    "clean": "rimraf static/dist",
    "build": "npm run clean && webpack --config ./webpack.config.js -p --progress --profile",
    "watch": "npm run clean && webpack-dev-server --content-base static/ --hot --inline --config webpack.config.js",
    "serve": "npm run build && serve static",
    "start": "npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/swamp-agr/visa-calculator.git"
  },
  "author": "Alexander C. Mingoia",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/swamp-agr/visa-calculator/issues"
  },
  "dependencies": {
    "preact": "^8.2.9",
    "preact-compat": "^3.18.0"
  },
  "devDependencies": {
    "bower": "^1.8.4",
    "purescript": "0.11.4",
    "purescript-psa": "0.5.1",
    "purs-loader": "^3.1.4",
    "rimraf": "^2.6.2",
    "serve": "^8.1.2",
    "webpack": "^4.12.0",
    "webpack-command": "^0.2.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
